// Constructor function for Person objects
function Person(first, last, age, eye) {
    this.firstName = first;
    this.lastName = last;
    this.age = age;
    this.eyeColor = eye;
}

// Create a Person object
var myFather = new Person("John", "Doe", 50, "blue");

function Event(date, title, author, part, place) {
    this.date = date;
    this.title = title;
    this.author = author;
    this.part = part;
    this.place = place;
}

var event1 = new Event("05.2012", "Sen Nocy Letniej", "B.Britten", "Pigwy", "Teatr Wielki");
var event2 = new Event("04.2013", "Koronacja Poppei", "")
